{
  "name": "db2-embedded-sql-syntax-c-cpp",
  "displayName": "Db2 Embedded SQL Syntax for C/C++",
  "description": "Syntax highlighing for embedded IBM Db2 SQL statements in C/C++ code",
  "repository": {
    "type": "git",
    "url": "https://github.com/spburtsev/db2-embedded-sql-syntax-c-cpp"
  },
  "license": "MIT",
  "publisher": "spburtsev",
  "icon": "images/icon.png",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "db2",
    "sql",
    "c",
    "c++",
    "syntax"
  ],
  "activationEvents": [
    "onLanguage:sqc",
    "onLanguage:sqC"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "sqc",
        "extensions": [".sqc"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "sqC",
        "extensions": [".sqC", ".sqx"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sqc",
        "scopeName": "source.sqc",
        "path": "./textmate-grammars/sqc.tmLanguage.json"
      },
      {
        "language": "sqC",
        "scopeName": "source.sqC",
        "path": "./textmate-grammars/sqx.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "sqc",
        "path": "./snippets.json"
      },
      {
        "language": "sqC",
        "path": "./snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
